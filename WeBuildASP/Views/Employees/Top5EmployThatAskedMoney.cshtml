@model IEnumerable<WeBuildASP.Models.LoanForEmploy>

@{
    ViewData["Title"] = "Statistics";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Top 5 Employ That Asked Money.</h3>
    </div>
    <div class="panel-body">
        <table class="table table-striped table-hover">
            <thead>
                <tr class="success">
                    <th>
                        @Html.DisplayNameFor(model => model.ID)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.L_F_AMOUNT)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.L_F_EMPLOY)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ID)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.L_F_AMOUNT)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.L_F_EMPLOY)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


