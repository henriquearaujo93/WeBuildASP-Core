@model IEnumerable<WeBuildASP.Models.Employee>

@{
    ViewData["Title"] = "Statistics";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Top 5 Late Employs</h3>
    </div>
    <div class="panel-body">
        <table class="table table-striped table-hover">
            <thead>
                <tr class="success">
                    <th>
                        @Html.DisplayNameFor(model => model.ID)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.E_NAME)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.E_LASTANAME)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.E_NACIONALITY)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.E_DTANAS)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.E_ACTIVE)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ID)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.E_NAME)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.E_LASTANAME)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.E_NACIONALITY)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.E_DTANAS)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.E_ACTIVE)
                        </td>
                    </tr>
                }
            </tbody>

        </table>


    </div>
</div>
