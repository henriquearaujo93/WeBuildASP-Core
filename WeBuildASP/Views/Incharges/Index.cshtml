@model IEnumerable<WeBuildASP.Models.Incharge>


@{
    ViewData["Title"] = "Incharges";
}

<h2>@ViewData["Title"]</h2>

<p>
    <a asp-action="Create" class="btn btn-default">Create New</a> <a asp-action="GoAdminMenu" class="btn btn-default">Home</a>
</p>

<table class="table table-striped table-hover">
    <thead>
        <tr class="success">
            <th>
                @Html.DisplayNameFor(model => model.I_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.I_LASTNAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.I_ACTIVE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.I_USER)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.I_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.I_LASTNAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.I_ACTIVE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.I_USER)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.ID">Edit</a>
                    <a asp-action="Details" asp-route-id="@item.ID">Details</a>
                    <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
                </td>
            </tr>
        }
    </tbody>

</table>
